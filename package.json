  "main": "index.js",
  "scripts": {
    "start": "nodemon ./server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildDev": "webpack --config ./config/webpack.dev.js && xcopy dist\\ .\\server\\public /E /Y",
    "buildProd": "webpack --config ./config/webpack.prod.js && xcopy dist\\ .\\server\\public /E /Y",
    "nodemonDevWP": "nodemon --watch src --ext \"*\" --exec \"npm run buildDev && xcopy dist\\ .\\server\\public /E /Y && del /S .\\server\\public\\*.css && del /S .\\server\\public\\*.js.map\"",
    "nodemonProdWP": "nodemon --watch src --ext \"*\" --exec \"npm run buildProd && xcopy dist\\ .\\server\\public /E /Y\"",
    "startDevServer": "concurrently \"npm:start\" \"npm:nodemonProdWP\""
  },
